{
    "name": "Contact Sheet for Google",
    "version": "1.15",
    "minimum_chrome_version": "29",
    "description": "Google contacts manager",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage", "identity", "https://www.google.com/*"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon_16.png"
    },
    "key":"",
    "oauth2": {
		"client_id": "798905190586-trm426m9fl504aioe0na75elb7colijp.apps.googleusercontent.com",
		"scopes":["https://www.google.com/m8/feeds/", "https://www.googleapis.com/auth/contacts.readonly"]
	}
}